    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-JavaFXScroll<TP2>",
            "request": "launch",
            "mainClass": "JavaFXScroll",
            "vmArgs": "--module-path ./lib/javafx-sdk-11.0.2/lib --add-modules javafx.controls,javafx.fxml",
            "projectName": "TP2"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Css2Bin<TP2>",
            "request": "launch",
            "mainClass": "com.sun.javafx.css.parser.Css2Bin",
            "projectName": "TP2"
        }
    ]
}